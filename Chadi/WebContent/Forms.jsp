                                                        <div class="input-icon right">
                                                            <i class="fa fa-tag"></i>
                                                            <h:inputText  value="#{ produitcontroler.p.login}"></ placeholder="" styleClass="form-control" ></h:inputText></div>
                                                    </div>
                                                    
                                                </div>
